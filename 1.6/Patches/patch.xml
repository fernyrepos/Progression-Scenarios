<Patch>    


<!-- BULK PATCHES -->

                <Operation Class="PatchOperationConditional">
                    <xpath>/Defs/FactionDef[techLevel="Spacer"]</xpath>
                    <match Class="PatchOperationAdd">
                        <xpath>/Defs/FactionDef[techLevel="Spacer"]/startingResearchTags</xpath>
                        <value>
                            <li>FernySpacerStart</li>
                        </value>
                    </match>
                </Operation>

<!-- RENAMES -->      
